class Solution {
    public int removeDuplicates(int[] nums) {
        int n=nums.length;
        int j=0;
        for(int i=0;i<n;i++)
        {
//for(int j=0;i<n;j++)
           // {
            if(nums[i]!=nums[j])
            {
                nums[++j]=nums[i];
            }
      //  }
        }
        return ++j;
    }
}
